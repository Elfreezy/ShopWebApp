@model Shop

@{ ViewBag.Title = Model.Title;
    ViewBag.Text = Model.Text; }

<div class="div-box">
    <h2>
        @Model.Title
    </h2>
</div>

<div class="div-box">
    @Html.Raw(Model.Subtitle)
</div>

<div class="div-box">
    @Html.Raw(Model.Phone)
</div>

<div class="div-box">
    @Html.Raw(Model.Text)
</div>


<h4>Товары</h4>
<ul>
    @foreach (var item in ViewBag.Products)
    {
<li>@item.Title | 123456</li>}
</ul>
